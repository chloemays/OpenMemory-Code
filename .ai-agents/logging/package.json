{
  "name": "@ai-agents/logging-tracing",
  "version": "1.0.0",
  "description": "Autonomous Logging and Runtime Execution Tracing System for AI-Agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:core && npm run build:api && npm run build:mcp",
    "build:core": "tsc --project tsconfig.core.json",
    "build:api": "tsc --project tsconfig.api.json",
    "build:mcp": "tsc --project tsconfig.mcp.json",
    "build:watch": "tsc --watch --project tsconfig.core.json",
    "clean": "rimraf dist",
    "dev": "npm run build:watch",
    "start:api": "node dist/api/server.js",
    "start:api:dev": "nodemon --watch dist dist/api/server.js",
    "test": "echo \"Tests not yet implemented\" && exit 0",
    "prepare": "npm run build"
  },
  "keywords": [
    "logging",
    "tracing",
    "instrumentation",
    "ast",
    "execution-flow",
    "ai-agents",
    "openmemory"
  ],
  "author": "OpenMemory + AI-Agents",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "typescript": "^5.3.3",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.5",
    "rimraf": "^5.0.5",
    "nodemon": "^3.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
